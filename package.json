{
  "name": "kamri-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "KAMRI - Fullstack E-commerce Monorepo",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:mobile": "turbo run dev --filter=mobile",
    "dev:server": "turbo run dev --filter=server",
    "dev:all": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:generate": "cd server && pnpm prisma generate",
    "db:push": "cd server && pnpm prisma db push",
    "db:seed": "cd server && pnpm prisma db seed"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "axios": "^1.12.2"
  }
}
